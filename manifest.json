{
  "manifest_version": 3,
  "name": "GPThreads",
  "version": "0.1",
  "description": "Extension test",

  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ]


}
